---
layout: "page"
---
<% content_for :head do %>
    A Seldomly Updated Website
<% end%>

<h2 class="h3">Recent Articles</h2>
<ul class="posts">
    <% blog.articles[0...10].each do |article| %>
        <li>
            <span><%= article.date.strftime('%b %e') %></span> &raquo; <%= link_to article.title, article %>
        </li>
    <% end %>
</ul>

